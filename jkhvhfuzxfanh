import random
schetchik=0
def pobeda(str1,str2):
    if str1==str2:
        return True
    else:
        return False
def teplo(ch1, ch2):
    result=False
    for zivra in ch1:
        for ziVra in ch2:
            if zivra==ziVra:
             result=True
    return result
zivra1=random.randint(0,9)
zivra2=random.randint(0,9)
while zivra1==zivra2:
    zivra2=random.randint(0,9)
zivra3=random.randint(0,9)
while zivra1==zivra3 or zivra2==zivra3:
    zivra3=random.randint(0,9)
chislo=str(zivra1)+str(zivra2)+str(zivra3)
chislo1=input('Введите 3-ёх значное число:')
if proverka1(chislo,chislo1)==True:
    schetchik=schetchik+1
